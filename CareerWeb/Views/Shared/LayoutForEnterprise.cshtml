<body>
    <header>
        <div class="container">
            <div class="row">
                <!--responsive-->
                <div class="responsive_menu_icon">
                    <i class="fas fa-bars"></i>
                    <span>MENU</span>
                </div>
                <!--Logo-->
                <div class="logo">
                    <div class="logo_box">
                        <img src="img/uet-1.pNg">
                    </div>
                </div>
                <!--Menu-->
                <div class="menu">
                    <div class="element">
                        <div class="element_box">
                            <i class="fas fa-search"></i>
                            <span>TÌM VIỆC LÀM</span>
                        </div>
                    </div>
                    <div class="element">
                        <div class="element_box">
                            <i class="fas fa-leaf"></i>
                            <span>CÔNG TY</span>
                        </div>
                    </div>
                    <div class="element">
                        <div class="element_box">
                            <i class="fas fa-list"></i>
                            <span>CẨM NANG VIỆC LÀM</span>
                        </div>
                    </div>
                    <div class="element">
                        <div class="element_box">
                            <i class="fas fa-file-alt"></i>
                            <span>TẠO CV</span>
                        </div>
                    </div>
                </div>
                <div class="user">
                    <!--Account-->
                    <div class="account">
                        <div class="log_in">
                            <i class="fas fa-user"></i>
                            <span>Đăng nhập</span>
                        </div>
                        <div class="registration">
                            <i class="fas fa-user-plus"></i>
                            <span>Đăng kí</span>
                        </div>
                    </div>
                    <!--Searching-->
                    <div class="searching">NHÀ TUYỂN DỤNG</div>
                </div>
            </div>
        </div>
        <div class="responsive_menu">
            <div class="res_element">
                <div class="res_element_box">
                    <i class="fas fa-search"></i>
                    <span>TÌM VIỆC LÀM</span>
                </div>
            </div>
            <div class="res_element">
                <div class="res_element_box">
                    <i class="fas fa-leaf"></i>
                    <span>CÔNG TY</span>
                </div>
            </div>
            <div class="res_element">
                <div class="res_element_box">
                    <i class="fas fa-list"></i>
                    <span>CẨM NANG VIỆC LÀM</span>
                </div>
            </div>
            <div class="res_element">
                <div class="res_element_box">
                    <i class="fas fa-file-alt"></i>
                    <span>TẠO CV</span>
                </div>
            </div>
            <div class="res_searching">
                <span>NHÀ TUYỂN DỤNG</span>
            </div>
        </div>
    </header>

    @RenderBody()
    <div class="content">
    </div>

